[alias]
	# git changed
	changed = show --pretty=\"format:\" --name-only
	# git what
	what = log --graph --decorate --oneline --color --stat
	# git where
	where = "!sh -c 'git log --graph --decorate --color --stat --grep=\"$1\"' -"
[color]
        ui = true
